<div class="container">
    <hr>
    <% articles.rows.forEach(article => { %>
        <div class="card">
            <div class="card-header">
                <h2><%= article.title %> </h2>
            </div>
            <div class="card-body">
                <a href="/<%=article.slug%>" class="btn btn-success">Ler Artigo</a>
            </div>
        </div>
        <br>
    <% }) %>  
</div>
